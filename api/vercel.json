{
    "version": 2,
    "buildCommand": "npx prisma generate && npx prisma db push --skip-generate",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}